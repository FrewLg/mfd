{% extends 'base.html.twig' %}

{% block title %}CoInvestigator index{% endblock %}

{% block body %}
    <h1>CoInvestigator index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for co_investigator in co_investigators %}
            <tr>
                <td>{{ co_investigator.id }}</td>
                <td>
                    <a href="{{ path('app_c_r_p_co_investigator_show', {'id': co_investigator.id}) }}">show</a>
                    <a href="{{ path('app_c_r_p_co_investigator_edit', {'id': co_investigator.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_c_r_p_co_investigator_new') }}">Create new</a>
{% endblock %}
