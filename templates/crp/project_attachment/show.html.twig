{% extends 'base.html.twig' %}

{% block title %}ProjectAttachment{% endblock %}

{% block body %}
    <h1>ProjectAttachment</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project_attachment.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ project_attachment.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ project_attachment.description }}</td>
            </tr>
            <tr>
                <th>DataAttached</th>
                <td>{{ project_attachment.dataAttached ? project_attachment.dataAttached|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateUpdated</th>
                <td>{{ project_attachment.dateUpdated ? project_attachment.dateUpdated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_c_r_p_project_attachment_index') }}">back to list</a>

    <a href="{{ path('app_c_r_p_project_attachment_edit', {'id': project_attachment.id}) }}">edit</a>

    {{ include('crp/project_attachment/_delete_form.html.twig') }}
{% endblock %}
