{% extends 'base.html.twig' %}

{% block title %}Funding Organization  {% endblock %}

{% block body %}
 
  <div class="row">
		<div class="col-lg-12">
			<div class="card">
				<div class="card-header">
					<div class="row align-items-center">
						<div class="col">
							<h4 class="card-title">
Funding Organization							</h4>
							Total: ({{funding_organizations|length}})
						</div>
						<!--end col-->
						<div class="col-auto">
							<a href="{{ path('app_c_r_p_funding_organization_new') }}" class='btn btn-primary btn-sm'>
								Create   new
							</a>
 
						</div>
						<!--end col-->
					</div>
				</div>
				<div class="card-body">
				
					<table class="table table-bordered table-striped">
           <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Logo</th>
                <th>CreatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for funding_organization in funding_organizations %}
            <tr>
                <td>{{ funding_organization.id }}</td>
                <td>{{ funding_organization.name }}</td>
                <td>{{ funding_organization.logo }}</td>
                <td>{{ funding_organization.createdAt ? funding_organization.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_c_r_p_funding_organization_show', {'id': funding_organization.id}) }}">show</a>
                    <a href="{{ path('app_c_r_p_funding_organization_edit', {'id': funding_organization.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

 


</div>
			</div>
		</div>
        {% endblock %}
