{% extends 'base.html.twig' %}

{% block title %}ApplicationFeedback index{% endblock %}

{% block body %}
    <h1>ApplicationFeedback index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Description</th>
                <th>CreatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for application_feedback in application_feedbacks %}
            <tr>
                <td>{{ application_feedback.id }}</td>
                <td>{{ application_feedback.description }}</td>
                <td>{{ application_feedback.createdAt ? application_feedback.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_application_feedback_show', {'id': application_feedback.id}) }}">show</a>
                    <a href="{{ path('app_application_feedback_edit', {'id': application_feedback.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_application_feedback_new') }}">Create new</a>
{% endblock %}
